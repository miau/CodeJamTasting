{
    "name": "Code Jam Tasting",
    "manifest_version": 2,
    "version": "0.13",
    "description": "Google Code Jam のスコアボードにソースプレビューボタンを追加",
    "content_scripts": [{
        "matches": [
            "https://code.google.com/codejam/contest/*/scoreboard*"
        ],
        "css": [
            "bootstrap.min.css",
            "prettify.css",
            "CodeJamTasting.css"
        ],
        "js": [
            "jquery.min.js",
            "prettify.js",
            "bootstrap.min.js",
            "jszip.min.js",
            "CodeJamTasting.js"
        ]
    }]
}
